{
	"name": "vue3-code-generator-js",
	"displayName": "vue3-code-generator-js",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
